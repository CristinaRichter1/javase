package nestedclasses;

public class StaticNestedOuterTest6 {
  static int data = 30;

  static class Inner {
    void msg() { System.out.println("data is " + data); }
  }
 
  public static void main(String args[]){
    StaticNestedOuterTest6.Inner obj=new StaticNestedOuterTest6.Inner();
    obj.msg();
  }
}


